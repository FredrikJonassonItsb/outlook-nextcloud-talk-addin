<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0"
           xmlns:mailappor="http://schemas.microsoft.com/office/mailappversionoverrides/1.0"
           xsi:type="MailApp">
  
  <Id>f8c7d9e2-4a6b-4f3e-9d8c-1a2b3c4d5e6f</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>ITSLyzer</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  
  <DisplayName DefaultValue="Nextcloud Talk for Outlook"/>
  <Description DefaultValue="Integrate Nextcloud Talk meetings and calendar with Outlook"/>
  
  <IconUrl DefaultValue="https://fredrikjonassonitsb.github.io/outlook-nextcloud-talk-addin/src/assets/icon-64.png"/>
  <HighResolutionIconUrl DefaultValue="https://fredrikjonassonitsb.github.io/outlook-nextcloud-talk-addin/src/assets/icon-128.png"/>
  <SupportUrl DefaultValue="https://github.com/FredrikJonassonItsb/outlook-nextcloud-talk-addin"/>
  
  <AppDomains>
    <AppDomain>https://fredrikjonassonitsb.github.io</AppDomain>
  </AppDomains>
  
  <Hosts>
    <Host Name="Mailbox"/>
  </Hosts>
  
  <Requirements>
    <Sets>
      <Set Name="Mailbox" MinVersion="1.1"/>
    </Sets>
  </Requirements>
  
  <FormSettings>
    <Form xsi:type="ItemRead">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://fredrikjonassonitsb.github.io/outlook-nextcloud-talk-addin/src/taskpane/taskpane.html"/>
        <RequestedHeight>250</RequestedHeight>
      </DesktopSettings>
    </Form>
  </FormSettings>
  
  <Permissions>ReadWriteItem</Permissions>
  
  <Rule xsi:type="RuleCollection" Mode="Or">
    <Rule xsi:type="ItemIs" ItemType="Appointment" FormType="Edit"/>
    <Rule xsi:type="ItemIs" ItemType="Appointment" FormType="ReadOrEdit"/>
  </Rule>
  
  <DisableEntityHighlighting>false</DisableEntityHighlighting>
  
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides" xsi:type="VersionOverridesV1_0">
    <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides/1.1" xsi:type="VersionOverridesV1_1">
      
      <Requirements>
        <bt:Sets DefaultMinVersion="1.5">
          <bt:Set Name="Mailbox"/>
        </bt:Sets>
      </Requirements>
      
      <Hosts>
        <Host xsi:type="MailHost">
          
          <DesktopFormFactor>
            <FunctionFile resid="Commands.Url"/>
            
            <ExtensionPoint xsi:type="AppointmentOrganizerCommandSurface">
              <OfficeTab id="TabDefault">
                <Group id="msgReadGroup">
                  <Label resid="GroupLabel"/>
                  
                  <Control xsi:type="Button" id="msgReadOpenPaneButton">
                    <Label resid="TaskpaneButton.Label"/>
                    <Supertip>
                      <Title resid="TaskpaneButton.Label"/>
                      <Description resid="TaskpaneButton.Tooltip"/>
                    </Supertip>
                    <Icon>
                      <bt:Image size="16" resid="Icon.16x16"/>
                      <bt:Image size="32" resid="Icon.32x32"/>
                      <bt:Image size="80" resid="Icon.80x80"/>
                    </Icon>
                    <Action xsi:type="ShowTaskpane">
                      <SourceLocation resid="Taskpane.Url"/>
                    </Action>
                  </Control>
                  
                  <Control xsi:type="Button" id="ActionButton">
                    <Label resid="ActionButton.Label"/>
                    <Supertip>
                      <Title resid="ActionButton.Label"/>
                      <Description resid="ActionButton.Tooltip"/>
                    </Supertip>
                    <Icon>
                      <bt:Image size="16" resid="Icon.16x16"/>
                      <bt:Image size="32" resid="Icon.32x32"/>
                      <bt:Image size="80" resid="Icon.80x80"/>
                    </Icon>
                    <Action xsi:type="ExecuteFunction">
                      <FunctionName>addNextcloudMeeting</FunctionName>
                    </Action>
                  </Control>
                  
                </Group>
              </OfficeTab>
            </ExtensionPoint>
          </DesktopFormFactor>
          
        </Host>
      </Hosts>
      
      <Resources>
        <bt:Images>
          <bt:Image id="Icon.16x16" DefaultValue="https://fredrikjonassonitsb.github.io/outlook-nextcloud-talk-addin/src/assets/icon-16.png"/>
          <bt:Image id="Icon.32x32" DefaultValue="https://fredrikjonassonitsb.github.io/outlook-nextcloud-talk-addin/src/assets/icon-32.png"/>
          <bt:Image id="Icon.80x80" DefaultValue="https://fredrikjonassonitsb.github.io/outlook-nextcloud-talk-addin/src/assets/icon-64.png"/>
        </bt:Images>
        <bt:Urls>
          <bt:Url id="Commands.Url" DefaultValue="https://fredrikjonassonitsb.github.io/outlook-nextcloud-talk-addin/src/commands/commands.html"/>
          <bt:Url id="Taskpane.Url" DefaultValue="https://fredrikjonassonitsb.github.io/outlook-nextcloud-talk-addin/src/taskpane/taskpane.html"/>
        </bt:Urls>
        <bt:ShortStrings>
          <bt:String id="GroupLabel" DefaultValue="Nextcloud Talk">
            <bt:Override Locale="sv-SE" Value="Nextcloud Talk"/>
          </bt:String>
          <bt:String id="TaskpaneButton.Label" DefaultValue="Show Taskpane">
            <bt:Override Locale="sv-SE" Value="Visa panelen"/>
          </bt:String>
          <bt:String id="ActionButton.Label" DefaultValue="Add Nextcloud Meeting">
            <bt:Override Locale="sv-SE" Value="Lägg till Nextcloud-möte"/>
          </bt:String>
        </bt:ShortStrings>
        <bt:LongStrings>
          <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Opens the Nextcloud Talk taskpane">
            <bt:Override Locale="sv-SE" Value="Öppnar Nextcloud Talk-panelen"/>
          </bt:String>
          <bt:String id="ActionButton.Tooltip" DefaultValue="Add a Nextcloud Talk meeting to this appointment">
            <bt:Override Locale="sv-SE" Value="Lägg till ett Nextcloud Talk-möte till denna händelse"/>
          </bt:String>
        </bt:LongStrings>
      </Resources>
      
    </VersionOverrides>
  </VersionOverrides>
  
</OfficeApp>

